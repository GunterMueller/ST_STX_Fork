preferredExtent
    explicitExtent notNil ifTrue:[
	^ explicitExtent
    ].

    "/ If I have a cached preferredExtent value..
    preferredExtent notNil ifTrue:[
	^ preferredExtent
    ].

    preferredExtent := (textMargin + (11 * wChar) + textMargin)@(textMargin + (10 * hChar) + textMargin).
    ^ preferredExtent

