updateTime
    |time timeString numDots|

    selfTest ifTrue:[
	time := lastTime.
	lastTime := lastTime addSeconds:30.
	(time hours ~= 0) ifTrue:[
	    (lastTime hours = 0) ifTrue:[
		selfTest := false.
	    ]
	].
	self pushEvent:#updateTimeAfterDelay.
	"/ time := Time fromString:'13:30'.
	"/ time := Time fromString:'23:58'.
    ] ifFalse:[
	time := Time now.
	"/ time := Time fromString:'13:30'.
	"/ time := Time fromString:'23:58'.
	"/ time := Time fromString:'23:41'.
	"/ time := Time fromString:'13:00'.
	"/ time := Time fromString:'13:05'.
	"/ time := Time fromString:'13:10'.
	"/ time := Time fromString:'13:15'.
	"/ time := Time fromString:'13:20'.
	"/ time := Time fromString:'13:25'.
	"/ time := Time fromString:'13:30'.
	"/ time := Time fromString:'14:30'.
	"/ time := Time fromString:'13:35'.
	"/ time := Time fromString:'13:40'.
	"/ time := Time fromString:'13:45'.
	"/ time := Time fromString:'13:50'.
	"/ time := Time fromString:'13:55'.
	"/ time := Time fromString:'14:55'.
	"/ time := Time fromString:'15:00'.
	"/ time := Time fromString:'9:45'.
	"/ time := Time fromString:'9:15'.
    ].

    numDots := self numDotsFor:time.
    (numDots ~= lastNumDotsShown) ifTrue:[
	numDots isNil ifTrue:[
	    "/ need full draw to clear dots.
	    self invalidate.
	    ^ self.
	].
	self drawMinuteDotsFor:time.
    ].

    timeString := self generateTextFor:time.
    (timeString ~= lastTimeString) ifTrue:[
	self drawText:timeString.
    ].

