!QlockTwoWatch class methodsFor:'startup'!

start
    self start:[:v | ]

    "Created: / 05-12-2011 / 14:22:17 / cg"
!

start:callBack
    |top v|

    top := StandardSystemView new.
    v := self origin:0.0@0.0 corner:1.0@1.0 in:top.
    v onKey:$q do:[ top destroy ].
    callBack value:v.
    top extent:(v preferredExtent).
    top label:'QLOCKTWO NANO'.
    top icon:(self appIcon2).
    top open.

    "Created: / 20-12-2011 / 19:30:43 / cg"
!

startFullScreen
    self startFullScreen:[:v ]

    "Created: / 19-12-2011 / 17:41:43 / cg"
!

startFullScreen:callBack
    |top v|

    top := StandardSystemView new.
    top bePopUpView.
    v := self origin:0.0@0.0 corner:1.0@1.0 in:top.
    v onKey:$q do:[ top destroy ].
    callBack value:v.
    top origin:0@0 extent:(Display extent).
    top label:'QLOCKTWO NANO'.
    top icon:(self appIcon2).
    top openModal.

    "Created: / 20-12-2011 / 19:30:23 / cg"
! !

!QlockTwoWatch methodsFor:'accessing'!

setColorScheme:schemeName
    "self open"

    DefaultScheme := schemeName.

    schemeName = #gray ifTrue:[
	bg := (Color red:70 green:70 blue:68).
	onColor := Color white.
	offColor := Color rgbValue:16r908A84.
	onFont := offFont asBold.
	self invalidate.
	^ self
    ].
    schemeName = #dark ifTrue:[
	bg := (Color redByte:32 greenByte:32 blueByte:29).
	onColor := Color yellow lightened.
	offColor := Color rgbValue:16r504844.
	onFont := offFont.
	onFont := offFont asBold.
	self invalidate.
	^ self
    ].
    schemeName = #iceBlue ifTrue:[
	bg := (Color red:5.88235294117647 green:53.3333333333333 blue:74.1176470588235).
	onColor := Color white.
	offColor := Color rgbValue:16r000066.
	onFont := offFont.
	onFont := offFont asBold.
	self invalidate.
	^ self
    ].
    schemeName = #black ifTrue:[
	bg := (Color grey:0.392156862745098).
	onColor := Color white.
	offColor := (Color grey:30).
	onFont := offFont asBold.
	self invalidate.
	^ self
    ].
    schemeName = #red ifTrue:[
	bg := (Color red:75.6862745098039 green:3.52941176470588 blue:13.7254901960784).
	onColor := Color white.
	offColor := Color rgbValue:16r707070.
	onFont := offFont asBold.
	self invalidate.
	^ self
    ].
    schemeName = #aluminumDark ifTrue:[
