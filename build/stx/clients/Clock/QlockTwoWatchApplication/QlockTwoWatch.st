!QlockTwoWatch methodsFor:'initialization'!

determineFontSizeFromExtent
    |goodFontSize|

    #(10 12 14 18 20 24 28 32 36 38 40 42 44 48 52 56 60 64) do:[:possibleSize |
	|font boldFont wFont hFont wChar hChar|

	font := Font family:'helvetica' face:"'bold'" 'medium' size:possibleSize.
	font := font onDevice:Display.
	boldFont := font asBold onDevice:Display.
	wFont := font width max:boldFont width.
	hFont := font height max:boldFont height.
	wChar := (wFont * 3.5) rounded.
	(textMargin + (wChar * 11) + textMargin) <= self width ifTrue:[
	    (textMargin + (hFont * 14) + textMargin) <= self height ifTrue:[
		goodFontSize := possibleSize.
	    ]
	]
    ].

    goodFontSize ~= fontSize ifTrue:[
	fontSize := goodFontSize.
	self initializeFont.
    ].

    "Created: / 20-12-2011 / 10:51:07 / cg"
!

initialize
    "QlockTwoWatch open"

    selfTest := false.

    keyActions := Dictionary new.
    autoFontSize := true "false".
    textMargin := 50.

    super initialize.
    autoFontSize ifTrue:[
	self determineFontSizeFromExtent
    ].
    self initializeFont.

    self extent:(textMargin + (11 * wChar) + textMargin)@(textMargin + (10 * hChar) + textMargin).

    self setColorScheme:(DefaultScheme ? #dark).
"/    bg := (Color red:83 green:83 blue:79).
