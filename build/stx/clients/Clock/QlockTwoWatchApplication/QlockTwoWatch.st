generateEnglishTimeTextFor:time language:lang
    |hour minute minuteTruncatedTo5 hr hrNext prefix|

    "
     self new generateTimeTextFor:(Time readFrom:'16:05') language:'en'
     self new generateTimeTextFor:(Time readFrom:'17:15') language:'en'
     self new generateTimeTextFor:(Time readFrom:'17:20') language:'en'
     self new generateTimeTextFor:(Time readFrom:'17:25') language:'en'
     self new generateTimeTextFor:(Time readFrom:'17:30') language:'en'
     self new generateTimeTextFor:(Time readFrom:'17:35') language:'en'
     self new generateTimeTextFor:(Time readFrom:'17:40') language:'en'
     self new generateTimeTextFor:(Time readFrom:'17:45') language:'en'
     self new generateTimeTextFor:(Time readFrom:'17:50') language:'en'
     self new generateTimeTextFor:(Time readFrom:'17:55') language:'en'
    "

    prefix := { it. is. }.

    hour := time hour12.
    minute := time minutes.
    minuteTruncatedTo5 := minute truncateTo:5.

    minuteTruncatedTo5 == 0 ifTrue:[
	hr := (digits1 at:hour).
	^ prefix , { hr. oclock }
    ].

    hr := (digits2 at:hour).
    hrNext := (digits2 at:hour+1).

    minuteTruncatedTo5 == 5 ifTrue:[
	^ prefix , { five. past. hr }
    ].
    minuteTruncatedTo5 == 10 ifTrue:[
	^ prefix , { ten. past. hr }
    ].
    minuteTruncatedTo5 == 15 ifTrue:[
	^ prefix , { quarter. past. hr }
    ].
    minuteTruncatedTo5 == 20 ifTrue:[
	^ prefix , { twenty. past. hr }
    ].
    minuteTruncatedTo5 == 25 ifTrue:[
	^ prefix , { twentyFive. past. hr }
    ].
    minuteTruncatedTo5 == 30 ifTrue:[
	^ prefix , { half. past. hr }
    ].
    minuteTruncatedTo5 == 35 ifTrue:[
	^ prefix , { twentyFive. to. hrNext }
    ].
    minuteTruncatedTo5 == 40 ifTrue:[
	^ prefix , { twenty. to. hrNext }
    ].
    minuteTruncatedTo5 == 45 ifTrue:[
	^ prefix , { a . quarter. to. hrNext }
    ].
    minuteTruncatedTo5 == 50 ifTrue:[
	^ prefix , { ten. to. hrNext }
    ].
    minuteTruncatedTo5 == 55 ifTrue:[
	^ prefix , { five. to. hrNext }
    ].
    self error.

    "
     self new generateText
