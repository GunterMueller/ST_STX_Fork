!QlockTwoWatch methodsFor:'drawing'!

drawMinuteDotsFor:aTime
    |dots mH mV x1 y1 x2 y2 x3 y3 x4 y4|

    dots := lastNumDotsShown := self numDotsFor:aTime.
    dots isNil ifTrue:[^ self].

    mH := ((self width - (11 * wChar)) // 2) max:0.
    mV := ((self height - (10 * hChar)) // 2) max:0.

    x1 := mH - 20.
    y1 := mV - 20.

    x2 := mH + (11 * wChar) + 20.
    y2 := mV - 20.

    x3 := mH + (11 * wChar) + 20.
    y3 := mV + (10 * hChar) + 20.

    x4 := mH - 20.
    y4 := mV + (10 * hChar) + 20.

    self paint:(dots > 0 ifTrue:[onColor] ifFalse:[offColor]).
    self fillCircleX:x1 y:y1 radius:3.

    self paint:(dots > 1 ifTrue:[onColor] ifFalse:[offColor]).
    self fillCircleX:x2 y:y2 radius:3.

    self paint:(dots > 2 ifTrue:[onColor] ifFalse:[offColor]).
    self fillCircleX:x3 y:y3 radius:3.

    self paint:(dots > 3 ifTrue:[onColor] ifFalse:[offColor]).
    self fillCircleX:x4 y:y4 radius:3.

    "Created: / 05-12-2011 / 13:02:42 / cg"
!

drawText:shownText
    |mH mV x0 x y0 y ascent nextWord wordsRemaining charsRemainingInWord inSpacing|

    lastTimeString := shownText.
    "/ Transcript showCR:shownText.

    wordsRemaining := shownText readStream.
    nextWord := wordsRemaining next.
    charsRemainingInWord := 0.
    inSpacing := false.
    ascent := self font ascent.

    mH := ((self width - (11 * wChar)) // 2) max:0.
    mV := ((self height - (10 * hChar)) // 2) max:0.

    x0 := mH.
    y0 := mV + ascent.

    y := y0.
    text do:[:row |
        |restRow|

        x := x0.
        restRow := row.
        inSpacing := false.
        row do:[:char |
            |charString wThisChar|

            charsRemainingInWord == 0 ifTrue:[
                "/ only look for next word after we have drawn one other character
                "/ (to avoid NACHVIER to be drawn in line 3)
                inSpacing ifTrue:[
                    inSpacing := false.
                ] ifFalse:[
                    (nextWord notNil and:[restRow startsWith:nextWord]) ifTrue:[
                        charsRemainingInWord := nextWord size.
                        nextWord := wordsRemaining nextOrNil.
                    ].
                ].
            ].

            self paint:bg.
            self fillRectangleX:x y:y-ascent width:wChar height:hChar.

            charsRemainingInWord > 0 ifTrue:[
                self paint:onColor.
                self font:onFont.
                charsRemainingInWord := charsRemainingInWord - 1.
                charsRemainingInWord == 0 ifTrue:[ inSpacing := true. ].
            ] ifFalse:[
                self paint:offColor.
                self font:offFont.
            ].
            charString := (String with:char).
            wThisChar := self font widthOf:charString.
            self displayString:charString x:(x + ((wChar - wThisChar) // 2)) y:y.
            x := x + wChar.
            restRow := restRow copyFrom:2.
        ].
        y := y + hChar.
    ].
    "/ self assert:(wordsRemaining atEnd).
    "/ self assert:(charsRemainingInWord == 0).

    "
     self new open
    "

