"/
"/    bg := (Color red:75 green:75 blue:72).
"/    offColor := (Color red:66 green:62 blue:60).

    "/ self langUageHolder value:(Smalltalk language).
    self initializeTexts.

    keyActions at:$t put:[ self startSelfTest ].

    "Created: / 04-12-2011 / 16:18:38 / cg"
!

initializeFont
    |usedFont wFont|

    fontSize isNil ifTrue:[ fontSize := 10 ].

    usedFont := Font family:'helvetica' face:"'bold'" 'medium' size:(fontSize ? 10).
    usedFont := usedFont onDevice:Display.
    
    usedFont notNil ifTrue:[
        self font:usedFont.
    ].
    onFont := offFont := (self font ? usedFont).

    onFont := onFont asBold onDevice:Display.

    wFont := onFont width max:offFont width.
    wChar := (wFont*3.5) rounded.
    hChar := wChar.

